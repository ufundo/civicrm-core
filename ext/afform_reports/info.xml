  <civix>
    <namespace>CRM/AfformReports</namespace>
    <format>24.09.1</format>
    <angularModule>afReports</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty@1.0.3</mixin>
    <mixin>ang-php@1.0.0</mixin>
  </mixins>
