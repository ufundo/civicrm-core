<?xml version="1.0"?>
<extension key="afform_reports" type="module">
  <file>afform_reports</file>
  <name>afform_reports</name>
  <description>Afform-based Reporting UI</description>
  <license>AGPL-3.0</license>
  <authors>
    <author>
      <name>benjamin</name>
      <email>ben@civicrm.org</email>
      <role>Maintainer</role>
    </author>
  </authors>
  <urls>
    <url desc="Main Extension Page">https://FIXME</url>
    <url desc="Documentation">https://FIXME</url>
    <url desc="Support">https://FIXME</url>
    <url desc="Licensing">https://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-11-11</releaseDate>
  <version>1.0.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.81</ver>
  </compatibility>
  <comments>Afform-based reporting UI</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
